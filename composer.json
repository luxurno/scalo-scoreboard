{
  "name": "luxurno/sportradar",
  "description": "Sport radar Scoreboard PHP Library",
  "require": {
    "php": ">=8.0.0"
  },
  "autoload": {
    "psr-4": {
      "Sportradar\\Library\\Scoreboard\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sportradar\\Library\\Scoreboard\\": [
